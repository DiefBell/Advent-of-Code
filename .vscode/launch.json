{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "TS/JS",
			"type": "bun",
			"internalConsoleOptions": "neverOpen",
			"request": "launch",
			"program": "${file}",
			"cwd": "${workspaceFolder}",
		},
		{
			"name": "C++",
			"type": "cppdbg",
			"request": "launch",
			"program": "${fileDirname}/bin/${fileBasenameNoExtension}.out",
			"cwd": "${fileDirname}",
			"preLaunchTask": "C/C++: g++ build active file",
			"stopAtEntry": false,
			"linux": {
				"MIMode": "gdb",
				"miDebuggerPath": "/usr/bin/gdb"
			}
		},
		{
			"name": "Python",
			"type": "debugpy",
			"internalConsoleOptions": "openOnSessionStart",
			"request": "launch",
			"program": "${file}",
			"cwd": "${fileDirname}",
			"console": "internalConsole"
		},
		{
			"name": "C# (.NET Core)",
			"type": "coreclr",
			"internalConsoleOptions": "openOnSessionStart",
			"request": "launch",
			"preLaunchTask": "Build C# Project",
			"program": "${fileDirname}/bin/Debug/net9.0/${fileDirnameBasename}.dll",
			"args": [],
			"cwd": "${fileDirname}",
			"stopAtEntry": false,
			"console": "internalConsole",
		},
		{
			"name": "Java",
			"type": "java",
			"internalConsoleOptions": "openOnSessionStart",
			"request": "launch",
			"mainClass": "${fileBasenameNoExtension}",
			"cwd": "${fileDirname}",
			"preLaunchTask": "Compile Java",
			"args": [],
			"console": "internalConsole"
		},
		{
			"name": "Go",
			"type": "go",
			"internalConsoleOptions": "openOnSessionStart",
			"request": "launch",
			"program": "${file}",
			"env": {},
			"args": [],
			"cwd": "${fileDirname}",
			"console": "internalConsole"
		},
		{
			"name": "R",
			"type": "R-Debugger",
			"request": "launch",
			"file": "${file}",
			"workingDirectory": "${fileDirname}"
		},
		{
			"type": "perl",
			"request": "launch",
			"name": "Perl",
			"program": "${file}",
			"cwd": "${fileDirname}",
			"stopOnEntry": false,
			"reloadModules": true
		},
		{
			"name": "Rust",
			"type": "lldb",
			"request": "launch",
			"program": "${fileDirname}/../target/debug/advent_of_code_2024",
			"args": [],
			"cwd": "${fileDirname}/..",
			"preLaunchTask": "cargo build",
			"internalConsoleOptions": "openOnFirstSessionStart"
		},
		{
			"name": "Fortran",
			"type": "cppdbg",
			"request": "launch",
			"internalConsoleOptions": "openOnFirstSessionStart",
			"program": "${fileDirname}/${fileBasenameNoExtension}.out",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${fileDirname}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			],
			"miDebuggerPath": "/usr/bin/gdb",
			"preLaunchTask": "Build Fortran",
			"miDebuggerArgs": "",
			"logging": {
				"moduleLoad": false,
				"trace": false,
				"engineLogging": false
			}
		},
		{
			"name": "Ruby",
			"type": "Ruby",
			"request": "launch",
			"program": "${file}",
			"cwd": "${fileDirname}",
			"internalConsoleOptions": "openOnSessionStart"
		},
	]
}